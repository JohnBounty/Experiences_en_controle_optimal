    "# Optimisation - la méthode de tir"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "jMDEulkv4qj_"
   },
   "outputs": [],
   "source": [
    "c_hat = 0.2\n",
    "range_of_c = np.linspace(c_hat, M, 222)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {
    "colab": {},
    "colab_type": "code",
    "id": "-pIQxgHL85Q4"
   },
   "outputs": [],
   "source": [
    "def shooting(func, c_range, target):\n",
    "    opt = 1e5\n",
    "    c_opt = 1e5\n",
    "\n",
    "    for c0 in c_range:\n",
    "        Z0 = np.array([x0, Dp(c0)])\n",
    "        Z, ts = Runge_Kutta_Four(func, Z0, t0, N, dt)\n",
    "\n",
    "        if np.abs(Z[-1, 1] - target) < opt:\n",
    "            opt = np.abs(Z[-1, 1] - target)\n",
    "            c_opt = c0\n",
    "    print(\"Shooting is over. Optimal value is {:.3f} found at c0 = {:.3f}.\".format(opt, c_opt))\n",
    "    return c_opt"
   ]
  },
  {
   "cell_type": "code",
